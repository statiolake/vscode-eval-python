{
  "name": "vscode-eval-python",
  "displayName": "Python Eval",
  "description": "Execute Python code in selections and replace with results",
  "version": "0.1.0",
  "publisher": "statiolake",
  "author": "statiolake <statiolake@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/statiolake/vscode-eval-python.git"
  },
  "bugs": {
    "url": "https://github.com/statiolake/vscode-eval-python/issues"
  },
  "homepage": "https://github.com/statiolake/vscode-eval-python#readme",
  "keywords": [
    "python",
    "eval",
    "execute",
    "selection",
    "replace"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "ui"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pythonEval.evaluateSelection",
        "title": "Evaluate Selected Python Expression",
        "category": "Python Eval"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Python Eval",
      "properties": {
        "pythonEval.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Path to Python executable",
          "scope": "machine"
        },
        "pythonEval.defaultImports": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "import math",
            "import os",
            "import sys",
            "import json",
            "import datetime",
            "from datetime import datetime, timedelta"
          ],
          "description": "Default imports to include in every execution"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
